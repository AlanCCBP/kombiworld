    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "debug": "~2.6.9",
    "dotenv": "^16.6.1",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1",
